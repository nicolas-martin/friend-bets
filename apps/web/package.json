{
  "name": "@friend-bets/web",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --web --port 9001",
    "dev-webpack": "expo export --platform web --dev && npx serve dist --listen 9001 --single",
    "dev-no-watch": "EXPO_NO_WEB_SETUP=true expo export:embed --platform web --dev && npx serve web-build --listen 9001",
    "build": "expo export --platform web",
    "build:prod": "expo export --platform web --output-dir dist --minify",
    "preview": "npx serve dist",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .expo dist node_modules/.cache"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.7.0",
    "@connectrpc/connect": "^1.4.0",
    "@connectrpc/connect-web": "^1.4.0",
    "@coral-xyz/anchor": "^0.29.0",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-community/datetimepicker": "7.7.0",
    "@react-navigation/native": "^6.1.9",
    "@solana/spl-token": "^0.3.9",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.32",
    "@solana/wallet-standard-wallet-adapter-base": "^1.1.4",
    "@solana/web3.js": "^1.87.6",
    "@tanstack/react-query": "^5.17.9",
    "bn.js": "^5.2.1",
    "expo": "~50.0.0",
    "expo-build-properties": "~0.11.1",
    "expo-clipboard": "~5.0.1",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.2",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.19.13",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/bn.js": "^5.1.5",
    "@types/node": "^24.3.0",
    "@types/react": "~18.2.14",
    "@types/react-dom": "~18.0.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "react-native-svg": "14.1.0"
  }
}
